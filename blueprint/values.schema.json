{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "applicationGroupDisplayName": {
      "type": "string",
      "title": "applicationGroupDisplayName",
      "default": "My Application Group"
    },
    "applicationGroupName": {
      "type": "string",
      "title": "applicationGroupName",
      "default": "my-application-group"
    },
    "applicationName": {
      "type": "string",
      "title": "applicationName",
      "default": "my-application"
    },
    "developersList": {
      "type": "array",
      "title": "developersList",
      "items": {
        "type": "string"
      }
    },
    "permissions": {
      "type": "array",
      "title": "permissions",
      "items": {
        "type": "string"
      }
    },
    "policyPrApprovers": {
      "type": "boolean",
      "title": "policyPrApprovers",
      "default": false
    }
  },
  "required": [
    "applicationGroupDisplayName",
    "applicationGroupName",
    "applicationName",
    "developersList",
    "permissions",
    "policyPrApprovers"
  ]
}
